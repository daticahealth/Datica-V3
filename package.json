    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "*",
    "cross-env": "^6.0.3",
    "css-loader": "^2.1.1",
    "headroom.js": "^0.10.3",
    "laravel-mix": "^4.1.4",
    "laravel-mix-purgecss": "^4.2.0",
    "lozad": "^1.14.0",
    "postcss": "^8.2.10",
    "postcss-at-rules-variables": "^0.1.9",
    "postcss-color-function": "^4.1.0",
    "postcss-each": "^0.10.0",
    "sass": "^1.23.3",
    "sass-loader": "^7.1.0",
    "tailwind-css-variables": "^2.0.3",
    "tailwindcss-alpha": "^0.1.4",
    "tailwindcss-export-config": "^2.0.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.41.2"
  },
